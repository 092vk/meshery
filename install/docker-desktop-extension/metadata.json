{
  "name": "meshery-docker-extension",
  "provider": "Docker Inc.",
  "icon": "docker.svg",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "extension-meshery-extension.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "Meshery",
      "src": "index.html",
      "root": "/app/ui/out",
      "backend": {
        "socket": "extension-meshery-extension.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "darwin/mesheryctl"
          }
        ]
      }
    ]
  }
}